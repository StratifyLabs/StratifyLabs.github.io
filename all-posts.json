---
layout: null
---
{
  "posts" : [

    {% for post in site.posts %}

    {"url":"{{ post.url }}"
    {% if post.category %}
      , "category":"{{ post.category }}"
    {% endif %}
    {% if post.tags %}
      , "tags":"{% for tag in post.tags %}{{ post.tags }},{% endfor %}"
    {% endif %}
    } {% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
